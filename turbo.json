{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "test": {
      "dependsOn": ["^lint", "^build"]
    },
    "type-check": {
      "outputs": []
    },
    "build#api": {
      "dependsOn": ["^build"]
    }
  }
}
